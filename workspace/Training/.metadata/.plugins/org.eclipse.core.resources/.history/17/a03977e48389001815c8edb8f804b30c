package com.training.basics;

public class DefaultHello implements HelloService {

	
	private String name;
	private String city;
	
	
	public DefaultHello() {
		
	}

	public DefaultHello(String name, String city) {
		super();
		this.name = name;
		this.city = city;
	}
	

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getCity() {
		return city;
	}

	public void setCity(String city) {
		this.city = city;
	}
//the values for this is given by configuration files
	@Override
	public String sayHello() {
		// TODO Auto-generated method stub
		return null;
	}

	@Override
	public String sayHello(String name, String city) {
		// TODO Auto-generated method stub
		
		String myStrs[]= {"hey how r u"goood morning and r you in};
		return null;
	}

}
